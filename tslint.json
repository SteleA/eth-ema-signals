{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "no-unnecessary-initializer": false,
    "max-classes-per-file": false,
    "object-literal-sort-keys": false,
    "no-debugger": false
  }
}
